{
  "name": "portfolio-prism",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:browser": "concurrently \"npm run dev:engine\" \"npm run dev\"",
    "dev:engine": "cd src-tauri/python && uv run python prism_headless.py --http",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "build:python": "python3 scripts/prism.py build",
    "start": "python3 scripts/prism.py all"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@tanstack/react-table": "^8.21.3",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-opener": "^2",
    "framer-motion": "^11.11.17",
    "lucide-react": "^0.456.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^2.12.7",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2",
    "@types/node": "^25.0.3",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
